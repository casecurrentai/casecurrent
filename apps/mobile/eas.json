{
  "cli": {
    "version": ">= 7.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://casecurrent.co"
      },
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://casecurrent.co"
      },
      "ios": {
        "simulator": true
      }
    },
    "android-internal": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "credentialsSource": "local"
      },
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://casecurrent.co"
      }
    },
    "production": {
      "distribution": "store",
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://casecurrent.co"
      },
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
